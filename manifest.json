{
  "manifest_version": 3,
  "name": "DungeonUI",
  "version": "1.0.0",
  "permissions": ["storage", "unlimitedStorage"],
  "content_scripts": [
    {
      "matches": [
        "https://play.aidungeon.com/*",
        "https://beta.aidungeon.com/*",
        "https://alpha.aidungeon.com/*",
        "http://localhost:5173/@crx/client-worker"
      ],
      "js": ["src/controls/events.ts"]
    }
  ]
}
