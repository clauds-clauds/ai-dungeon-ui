{
  "manifest_version": 3,
  "name": "DungeonUI",
  "version": "1.0.3",
  "permissions": ["storage", "unlimitedStorage"],
  "icons": {
    "16": "icons/batsy_16.png",
    "32": "icons/batsy_32.png",
    "64": "icons/batsy_64.png",
    "96": "icons/batsy_96.png",
    "128": "icons/batsy_128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://play.aidungeon.com/*", "https://beta.aidungeon.com/*", "http://localhost:5173/*"],
      "js": ["src/content/snooze.ts", "src/content/events.ts"]
    }
  ]
}
